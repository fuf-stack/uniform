{
  "name": "@fuf-stack/pixel-utils",
  "description": "fuf pixels helper utils",
  "author": "Hannes Tiede",
  "version": "0.4.3",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "./dist"
  ],
  "homepage": "https://github.com/fuf-stack/pixels/tree/main/packages/pixel-utils",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "tailwind",
    "utils"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fuf-stack/pixels.git",
    "directory": "packages/pixel-utils"
  },
  "bugs": {
    "url": "https://github.com/fuf-stack/pixels/issues"
  },
  "scripts": {
    "build": "tsup --config node_modules/@repo/tsup-config/config.ts",
    "prepack": "rm -rf ./dist && pnpm build"
  },
  "dependencies": {
    "@nextui-org/theme": "2.4.4",
    "classnames": "2.5.1",
    "tailwind-merge": "2.6.0",
    "tailwind-variants": "0.3.0"
  },
  "devDependencies": {
    "@repo/tsup-config": "workspace:*"
  }
}
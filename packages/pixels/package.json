    "@nextui-org/modal": "2.2.6",
    "@nextui-org/popover": "2.3.8",
    "@nextui-org/scroll-shadow": "2.3.4",
    "@nextui-org/system": "2.4.5",
    "@nextui-org/table": "2.2.7",
    "@nextui-org/theme": "2.4.4",
    "@nextui-org/tabs": "2.2.6",
    "@nextui-org/tooltip": "2.2.6",
    "framer-motion": "11.15.0",
    "next-themes": "0.4.4",
    "react-icons": "5.4.0"
  },
  "devDependencies": {
    "@repo/storybook-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/tsup-config": "workspace:*",
    "@repo/vite-config": "workspace:*",
    "@types/debug": "4.1.12",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@uiw/react-json-view": "2.0.0-alpha.30",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  }
}
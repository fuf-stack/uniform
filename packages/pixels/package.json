    "./ScrollShadow": {
      "types": "./dist/ScrollShadow/index.d.ts",
      "import": "./dist/ScrollShadow/index.js",
      "require": "./dist/ScrollShadow/index.cjs"
    },
    "./Table": {
      "types": "./dist/Table/index.d.ts",
      "import": "./dist/Table/index.js",
      "require": "./dist/Table/index.cjs"
    },
    "./Tabs": {
      "types": "./dist/Tabs/index.d.ts",
      "import": "./dist/Tabs/index.js",
      "require": "./dist/Tabs/index.cjs"
    },
    "./Tooltip": {
      "types": "./dist/Tooltip/index.d.ts",
      "import": "./dist/Tooltip/index.js",
      "require": "./dist/Tooltip/index.cjs"
    }
  },
  "files": [
    "./dist"
  ],
  "homepage": "https://github.com/fuf-stack/pixels/tree/main/packages/pixels",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "components",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fuf-stack/pixels.git",
    "directory": "packages/pixels"
  },
  "bugs": {
    "url": "https://github.com/fuf-stack/pixels/issues"
  },
  "scripts": {
    "build": "tsup --config node_modules/@repo/tsup-config/config.ts --dts-resolve",
    "prepack": "rm -rf ./dist && pnpm build",
    "test": "vitest ./src"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@fuf-stack/pixel-utils": "workspace:*",
    "@nextui-org/avatar": "2.2.5",
    "@nextui-org/accordion": "2.2.6",
    "@nextui-org/badge": "2.2.4",
    "@nextui-org/breadcrumbs": "2.2.5",
